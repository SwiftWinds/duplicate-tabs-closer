{
  "manifest_version": 3,
  "name": "Duplicate Tabs Closer (SwiftWinds [Peuj])",
  "author": "SwiftWinds <- Peuj",
  "permissions": ["tabs", "webNavigation", "storage", "background"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "browser_style": false,
    "default_icon": {
      "16": "images/auto_close_16.png",
      "32": "images/auto_close_32.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Duplicate Tabs Closer"
  },
  "commands": {
    "close-duplicate-tabs": {
      "suggested_key": {
        "default": "Alt+Shift+W"
      },
      "description": "Close all duplicate tabs"
    },
    "toggle-close-automatically": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Toggle close automatically"
    }
  },
  "default_locale": "en",
  "description": "Duplicate Tabs Closer",
  "icons": {
    "48": "images/auto_close_32.png"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "optionPage/optionPage.html"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.0.0"
}
